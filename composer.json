{
    "name": "itseasy/laminas-db",
    "description": "Laminas Database Wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wiryono Lau",
            "email": "wiryonolau@gmail.com"
        }
    ],
    "config": {
        "platform" : {
            "ext-intl" : "1"
        }
    },
    "require": {
        "php" : "^7.4",
        "laminas/laminas-db": "^2.13",
        "laminas/laminas-hydrator": "^4.3",
        "laminas/laminas-log": "^2.15"
    },
    "autoload" : {
        "psr-4" : {
            "Itseasy\\Database\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Itseasy\\DatabaseTest\\" : "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "itseasy/slim-app" : "dev-main"
    }
}
